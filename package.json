{
  "name": "seamless-interface",
  "private": true,
  "version": "0.0.0",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "start": "vite",
    "test:watch": "vitest watch",
    "preview": "vite preview",
    "switch-tailwind-config": "node switch-config.js",
    "dev": "npm run switch-tailwind-config && vite --port 5174",
    "compile": "tsc -p tsconfig.build.json",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 10",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix --no-cache",
    "prebuild": "cross-env NODE_ENV=production npm run switch-tailwind-config && npm run lint && npm run compile",
    "build": "cross-env NODE_ENV=production npm run prebuild && node --max-old-space-size=8192 ./node_modules/vite/bin/vite.js build",
    "anvil": "node run-anvil.mjs",
    "cypress-open": "concurrently \"npm run anvil\" \"npx cypress open\" --kill-others",
    "codegen": "graphql-codegen"
  },
  "dependencies": {
