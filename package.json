    "@tanstack/react-query": "^5.17.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@uidotdev/usehooks": "^2.4.1",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "ethereum-blockies-base64": "^1.0.2",
    "graphql": "^16.10.0",
